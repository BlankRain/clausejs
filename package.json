{
  "name": "specky-src",
  "version": "0.0.22",
  "description": "A powerful & flexible validation and conformation library for your JavaScript data and functions.",
  "main": "packages/specky/dist/specky.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "dev": "npm run test:watch",
    "dev-docsite": "webpack-dev-server --content-base ./docs --inline --host 0.0.0.0",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --watch",
    "build": "webpack && cross-env PROD_ENV=1 webpack && npm run build-docsite",
    "build-docsite": "babel-node scripts/build-docs.js",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speckyjs/specky.git"
  },
  "keywords": [
    "babel",
    "testing",
    "spec",
    "javascript",
    "specky",
    "validation",
    "schema"
  ],
  "author": {
    "name": "Xiyang Chen",
    "email": "settinghead@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/speckyjs/specky/issues"
  },
  "homepage": "https://github.com/speckyjs/specky#readme",
  "dependencies": {
    "get-function-name-x": "^1.0.11",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "lodash": "^4.17.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "bootstrap": "4.0.0-alpha.5",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.1.1",
    "eslint-config-idiomatic": "^2.1.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "mocha": "^3.0.2",
    "mocha-testcheck": "^0.1.2",
    "raw-loader": "^0.5.1",
    "showdown": "^1.5.4",
    "style-loader": "^0.13.1",
    "webpack": "2.2.0-rc.2"
  }
}
